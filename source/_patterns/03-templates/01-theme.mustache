<div class="container">
	{{> organisms-sidebar }}
	<div class="page" id="page">
		{{> organisms-header }}
		<div class="main">
			<div class="layoutContainer gridContainer">
				<div class="column column--full">
					<header class="article-header">
						<h1>Marketing</h1>
					</header>
				</div>
			</div>
			<div class="layoutContainer gridContainer">
				<div class="column column--twoThird">
					<h2 class="block-title h4">Veel gezocht</h2>
					<ul class="labelList">
						<li class="labelList-item">
							<a href="#" class="labelList-link">
								<span class="labelList-text"><span>
									Maak een concurrentieanalyse
								</span></span>
							</a>
